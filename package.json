{
  "name": "quantract",
  "private": true,
  "scripts": {
    "install:all": "pnpm install",
    "build": "turbo run build",
    "build:all": "turbo run build",
    "lint:all": "turbo run lint",
    "typecheck:all": "turbo run typecheck",
    "dev:crm": "pnpm --filter @quantract/crm run dev",
    "dev:marketing": "pnpm --filter @quantract/marketing run dev",
    "dev:certificates": "pnpm --filter @quantract/certificates run dev",
    "dev:tools": "pnpm --filter @quantract/tools run dev",
    "dev:engineer": "pnpm --filter engineer run start",
    "build:crm": "pnpm --filter @quantract/crm run build",
    "build:marketing": "pnpm --filter @quantract/marketing run build",
    "build:certificates": "pnpm --filter @quantract/certificates run build",
    "build:tools": "pnpm --filter @quantract/tools run build"
  },
  "packageManager": "pnpm@10.28.0",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@types/leaflet": "^1.9.21",
    "openai": "^6.17.0"
  },
  "devDependencies": {
    "dotenv": "^17.2.3",
    "turbo": "^2.8.3",
    "typescript": "^5.9.3"
  },
  "pnpm": {
    "overrides": {
      "lucide-react": "0.562.0",
      "tailwind-merge": "^3.4.0"
    },
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "@sentry/cli",
      "esbuild",
      "prisma",
      "sharp",
      "unrs-resolver"
    ]
  }
}
