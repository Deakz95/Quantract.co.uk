{
  "name": "@quantract/crm",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "build": "npm run prisma:generate && next build",
    "start": "node scripts/with-neon-conn.mjs -- next start -p 3000",
    "start:local": "next start -p 3000",
    "worker": "tsx src/lib/server/queue/worker.ts",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "test:e2e": "playwright test tests/playwright/e2e-*.spec.ts",
    "test:e2e:ui": "playwright test tests/playwright/e2e-*.spec.ts --ui",
    "test:smoke": "playwright test tests/playwright/e2e-*.spec.ts --project=chromium",
    "e2e:staging": "playwright test --project=chromium --reporter=list --config=playwright.e2e.config.ts",
    "e2e:auth": "playwright test --project=chromium --grep @auth --reporter=list --config=playwright.e2e.config.ts",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test": "vitest run",
    "smoke": "tsx scripts/smoke/run-smoke-tests.ts",
    "smoke:staging": "tsx scripts/smoke/run-staging-smoke.ts",
    "qa:staging": "tsx scripts/qa/feature-inventory-smoke.ts",
    "qa:no-leaks": "tsx scripts/qa/no-uuid-leaks.ts",
    "qa:purge-test-data": "tsx scripts/qa/purge-test-data.ts",
    "qa:api:full": "tsx scripts/qa/api-full-sweep.ts",
    "qa:backfill-quote-numbers": "tsx scripts/qa/backfill-quote-numbers.ts",
    "qa:stock-consume": "tsx scripts/qa/stock-consume-smoke.ts",
    "qa:security-audit": "tsx scripts/qa/security-audit.ts",
    "qa:backfill-site-latlng": "tsx scripts/qa/backfill-site-latlng.ts",
    "prisma:generate": "prisma generate",
    "prisma:validate": "prisma validate",
    "prisma:migrate:deploy": "node scripts/with-neon-conn.mjs -- prisma migrate deploy",
    "prisma:migrate:deploy:safe": "node scripts/with-neon-conn.mjs -- node scripts/safe-migrate.mjs",
    "prisma:migrate:deploy:local": "prisma migrate deploy",
    "prisma:migrate:fix": "node scripts/with-neon-conn.mjs -- node scripts/fix-migrations.mjs",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx prisma/seed.ts",
    "db:setup": "npm run prisma:generate && npm run prisma:migrate:dev && npm run prisma:seed",
    "docker:up": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:logs": "docker-compose -f docker-compose.dev.yml logs -f",
    "docker:reset": "docker-compose -f docker-compose.dev.yml down -v && docker-compose -f docker-compose.dev.yml up -d"
  },
  "prisma": {
    "schema": "./prisma/schema.prisma",
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "@neondatabase/auth": "latest",
    "@prisma/client": "^6.0.0",
    "@quantract/shared": "*",
    "@radix-ui/react-slot": "^1.2.4",
    "@sentry/nextjs": "latest",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "20.17.19",
    "@types/react": "18.2.79",
    "@types/react-dom": "18.2.25",
    "autoprefixer": "^10.4.0",
    "bcryptjs": "^3.0.3",
    "bullmq": "^5.66.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jszip": "^3.10.1",
    "@types/leaflet": "^1.9.21",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.562.0",
    "next": "16.1.0",
    "next-auth": "^4.24.13",
    "next-pwa": "^5.6.0",
    "openai": "^4.104.0",
    "pdf-lib": "^1.17.1",
    "postcss": "^8.4.0",
    "qrcode": "^1.5.4",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-leaflet": "^5.0.0",
    "redis": "^4.7.0",
    "resend": "^4.8.0",
    "stripe": "^16.12.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jszip": "^3.4.1",
    "@types/qrcode": "^1.5.6",
    "@vitest/coverage-v8": "^2.1.8",
    "babel-plugin-react-compiler": "1.0.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.0.0",
    "eslint-config-next": "16.1.0",
    "jsdom": "^25.0.1",
    "playwright": "^1.50.0",
    "prisma": "^6.0.0",
    "tsx": "^4.19.0",
    "typescript": "^5.9.3",
    "vitest": "^2.1.8"
  }
}
